{
  "name": "parkingmate",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "description": "ParkingMate - Smart parking management system",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "lint:fix:staged": "biome check --fix --unsafe --no-errors-on-unmatched --max-diagnostics=400 --verbose --staged ./**/*.{js,ts,jsx,tsx}",
    "openapi:generate:build": "pnpm --filter @parkingmate/api-client api:generate && pnpm --filter @parkingmate/api-client build",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:generate": "dotenv -e .env -- turbo run db:generate",
    "db:migrate": "dotenv -e .env -- turbo run db:migrate",
    "db:studio": "dotenv -e .env -- turbo run db:studio",
    "api:generate": "turbo run api:generate",
    "prepare": "node -e \"try { require('husky') } catch (e) { if (e.code !== 'MODULE_NOT_FOUND') throw e }\" || husky || true"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "dotenv-cli": "^10.0.0",
    "husky": "^9.1.7",
    "turbo": "^2.3.3",
    "typescript": "~5.8.3"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.19.0"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "pnpm": "^10.19.0"
  }
}
