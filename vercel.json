  "installCommand": "npm install",
  "buildCommand": "npm run build -w @parkingmate/api-client && npm run build -w @parkingmate/ui",
  "outputDirectory": "apps/web/dist",
  "ignoreCommand": "exit 0",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}